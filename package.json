{
  "name": "hsl-map-publisher",
  "version": "1.1.2",
  "description": "HSL Map Publisher",
  "main": "index.js",
  "scripts": {
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "prettier": "prettier \"src/**/*.{js,css}\" \"scripts/**/*.js\" \"migrations/**/*.js\" \"seeds/**/*.js\" \"*.js\" --write",
    "lint": "eslint \"src/**/*.js\" \"scripts/**/*.js\"",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "serve": "serve -l 5000 dist",
    "start": "webpack serve -c ./webpack.dev.js",
    "start:production": "serve -l 5000 dist",
    "server": "node -r dotenv/config scripts/server",
    "server:production": "node -r dotenv/config scripts/server.js",
    "worker": "node -r dotenv/config scripts/worker",
    "worker:production": "./fonts.sh && fc-cache -f -v && node -r dotenv/config scripts/worker.js",
    "knex": "PG_CONNECTION_STRING=postgres://postgres:postgres@localhost:5432/postgres knex",
    "docker:build": "docker build -t hsl-map-publisher .",
    "docker:run": "docker run -d -p 4000:4000 --name publisher -v $(pwd)/output:/output -v $(pwd)/fonts:/fonts --link publisher-postgres -e \"PG_CONNECTION_STRING=postgres://postgres:postgres@publisher-postgres:5432/postgres\" --shm-size=1G hsl-map-publisher",
    "docker:stop": "docker stop publisher",
    "docker:rm": "docker rm publisher",
    "docker:rebuild": "yarn run docker:stop && yarn run docker:rm && yarn run docker:build && yarn run docker:run",
    "docker:logs": "docker logs publisher",
    "prepare": "husky install",
    "test": "node -r dotenv/config test/tests"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HSLdevcom/hsl-map-publisher.git"
  },
  "author": "",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/HSLdevcom/hsl-map-publisher/issues"
  },
  "homepage": "https://github.com/HSLdevcom/hsl-map-publisher#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.24.5",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "cross-env": "^5.1.1",
    "css-loader": "~6",
    "dotenv-webpack": "^8.1.0",
    "eslint": "~8",
    "eslint-config-airbnb": "~19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^4",
    "eslint-webpack-plugin": "^4.1.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.2",
    "prettier": "^3.2.5",
    "style-loader": "~4.0.0",
    "webpack-cli": "~5.1.4",
    "webpack-dev-server": "~5.0.4",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@azure/storage-blob": "^10.4.0",
    "@koa/cors": "^5.0.0",
    "@turf/turf": "^6.5.0",
    "bullmq": "^1.86.2",
    "cheerio": "1.0.0-rc.12",
    "classnames": "^2.2.5",
    "dotenv": "^8.1.0",
    "fs-extra": "9.0.0",
    "graphql": "^16.8.1",
    "haversine": "^1.1.1",
    "hsl-map-style": "hsldevcom/hsl-map-style#master",
    "ioredis": "^5.0.6",
    "knex": "^2.0.0",
    "koa": "^2.5.3",
    "koa-body": "^6.0.1",
    "koa-json-body": "^5.3.0",
    "koa-router": "^12.0.1",
    "koa-session": "^6.4.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-fetch": "~2",
    "p-map": "^1.2.0",
    "pdf-merger-js": "^3.4.0",
    "pg": "^8.7.3",
    "prop-types": "^15.6.0",
    "qrcode": "^1.5.4",
    "puppeteer": "^22.9.0",
    "qs": "^6.5.2",
    "react-apollo": "^2.0.1",
    "react-csv": "^2.2.2",
    "react": "~18.2",
    "react-dom": "~18.2",
    "react-measure": "^2.5.2",
    "recompose": "^0.30.0",
    "segseg": "^1.0.0",
    "serve": "^14.2.3",
    "uuid": "^3.1.0",
    "viewport-mercator-project": "^4.1.1",
    "webpack": "^5.91.0",
    "webpack-merge": "^4.1.3"
  }
}
