{
  "name": "hsl-map-publisher",
  "version": "1.1.5",
  "description": "HSL Map Publisher",
  "main": "index.js",
  "scripts": {
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "prettier": "prettier \"src/**/*.{js,css}\" \"scripts/**/*.js\" \"migrations/**/*.js\" \"seeds/**/*.js\" \"*.js\" --write",
    "lint": "eslint \"src/**/*.js\" \"scripts/**/*.js\" --fix",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "serve": "serve -l 5000 dist",
    "start": "webpack-serve ./webpack.dev.js",
    "start:production": "serve -l 5000 dist",
    "server": "node -r dotenv/config scripts/server",
    "server:production": "node -r dotenv/config scripts/server.js",
    "worker": "node -r dotenv/config scripts/worker",
    "worker:production": "./fonts.sh && fc-cache -f -v && node -r dotenv/config scripts/worker.js",
    "knex": "PG_CONNECTION_STRING=postgres://postgres:postgres@localhost:5432/postgres knex",
    "docker:build": "docker build -t hsl-map-publisher .",
    "docker:run": "docker run -d -p 4000:4000 --name publisher -v $(pwd)/output:/output -v $(pwd)/fonts:/fonts --link publisher-postgres -e \"PG_CONNECTION_STRING=postgres://postgres:postgres@publisher-postgres:5432/postgres\" --shm-size=1G hsl-map-publisher",
    "docker:stop": "docker stop publisher",
    "docker:rm": "docker rm publisher",
    "docker:rebuild": "yarn run docker:stop && yarn run docker:rm && yarn run docker:build && yarn run docker:run",
    "docker:logs": "docker logs publisher",
    "prepare": "husky install",
    "test": "node -r dotenv/config test/tests"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HSLdevcom/hsl-map-publisher.git"
  },
  "author": "",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/HSLdevcom/hsl-map-publisher/issues"
  },
  "homepage": "https://github.com/HSLdevcom/hsl-map-publisher#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "react-refresh": "^0.14.0",
    "webpack-cli": "^6.0.1",
    "webpack-serve": "^4.0.0",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.27.0",
    "@koa/cors": "^5.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@turf/turf": "^7.2.0",
    "apollo-cache-inmemory": "^1.1.1",
    "apollo-client": "^2.0.3",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "bullmq": "^5.56.5",
    "cheerio": "=1.0.0-rc.3",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "dotenv": "^17.2.0",
    "dotenv-webpack": "^8.1.1",
    "fs-extra": "11.3.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.5.0",
    "haversine": "^1.1.1",
    "hsl-map-style": "hsldevcom/hsl-map-style#master",
    "html-webpack-plugin": "^5.6.3",
    "ioredis": "^5.0.6",
    "knex": "^3.1.0",
    "koa": "^3.0.0",
    "koa-body": "^6.0.1",
    "koa-json-body": "^5.3.0",
    "koa-router": "^7.4.0",
    "koa-session": "6.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-fetch": "3.3.2",
    "p-map": "^7.0.3",
    "pdf-merger-js": "^5.1.2",
    "pg": "^8.7.3",
    "prop-types": "^15.6.0",
    "puppeteer": "^13.7.0",
    "qrcode": "^1.5.4",
    "qs": "^6.5.2",
    "raw-loader": "^4.0.2",
    "react": "19.1.0",
    "react-apollo": "^3.1.5",
    "react-csv": "^2.2.2",
    "react-dom": "19.1.0",
    "react-measure": "^2.5.2",
    "recompose": "^0.30.0",
    "segseg": "^1.0.0",
    "serve": "^14.2.4",
    "style-loader": "^4.0.0",
    "uuid": "^11.1.0",
    "viewport-mercator-project": "^7.0.4",
    "webpack": "^5.100.2",
    "webpack-merge": "^6.0.1"
  }
}
